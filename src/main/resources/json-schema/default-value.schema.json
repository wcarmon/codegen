{
  "$id": "https://wcarmon.com/schemas/codegen/default-value.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "anyOf": [
    {
      "type": "null"
    },
    {
      "additionalProperties": false,
      "properties": {
        "value": {
          "anyOf": [
            {
              "type": "boolean"
            },
            {
              "type": "number"
            },
            {
              "maxLength": 2048,
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        }
      },
      "type": "object"
    }
  ],
  "description": "Default value, allows null, absent, non-null values"
}
