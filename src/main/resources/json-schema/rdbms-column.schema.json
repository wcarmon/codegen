{
  "$id": "https://wcarmon.com/schemas/codegen/rdbms-column.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "extra config for RDBMS Columns",
  "properties": {
    "autoIncrement": {
      "type": "boolean"
    },
    "overrideDefaultValue": {
      "description": "literal for default value, wrap strings in single quotes",
      "maxLength": 64,
      "minLength": 1,
      "type": "string"
    },
    "overrideSerde": {
      "$ref": "serde.schema.json"
    },
    "overrideTypeLiteral": {
      "description": "Allows custom DB type, replaces auto-derived DB type",
      "maxLength": 64,
      "minLength": 1,
      "type": "string"
    },
    "varcharLength": {
      "exclusiveMinimum": 0,
      "maximum": 10000,
      "type": "integer"
    }
  },
  "propertyNames": {
    "pattern": "^[$]?[A-Za-z_][A-Za-z0-9]*$"
  },
  "required": [],
  "title": "Column config",
  "type": "object"
}
