{
  "$schema": "https://wcarmon.com/schemas/codegen/field.schema.json",
  "golang": {
    "overrideEffectiveType": "string"
  },
  "java": {
    "overrideProtobufSerde": {
      "deserializeTemplate": "UBUuid.parse(%s)",
      "serializeTemplate": "%s.toString()"
    },
    "overrideRDBMSSerde": {
      "deserializeTemplate": "UBUuid.parse(%s)",
      "serializeTemplate": "%s.toString()"
    }
  },
  "kotlin": {
    "overrideProtobufSerde": {
      "deserializeTemplate": "UBUuid.parse(%s)",
      "serializeTemplate": "%s.toString()"
    },
    "overrideRDBMSSerde": {
      "deserializeTemplate": "UBUuid.parse(%s)",
      "serializeTemplate": "%s.toString()"
    }
  },
  "name": "owner",
  "protobuf": {
    "overrideEffectiveType": "string"
  },
  "rdbms": {
    "validation": {
      "minSize": 16,
      "requireNotBlank": true,
      "requireTrimmed": true
    },
    "varcharLength": 36
  },
  "type": "UBUuid"
}
